{
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"description": "A speed dial functionally similar to the one used in Vivaldi.",
	"icons": {
		"128": "icons/icon128.png",
		"48": "icons/icon48.png",
		"16": "icons/icon16.png"
	},
	"manifest_version": 2,
	"name": "Simple Speed Dial",
	"options_page": "options.html",
	"permissions": ["background", "bookmarks", "storage", "tabs"],
	"background": {
		"scripts": ["javascript/eventPage.js"],
		"persistent": false
	},
	"update_url": "http://clients2.google.com/service/update2/crx",
	"version": "2.5.0"
}
